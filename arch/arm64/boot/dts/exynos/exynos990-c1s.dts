/dts-v1/;

#include "exynos990.dtsi"

/ {
	model = "Samsung Galaxy Note20";
	compatible = "samsung,c1s", "samsung,exynos990";

    memory@80000000 {
		device_type = "memory";
		reg = <0 0x90000000 0 0x8000000>;
	};

    aliases {
	};

	chosen {
		bootargs = "earlycon=simplefb,0xF1000000,1080x2400x32 console=simplefb";

		#address-cells = <2>;
		#size-cells = <2>;
		ranges;
        stdout-path = "framebuffer0";

		framebuffer0: framebuffer@9d400000 {
			compatible = "simple-framebuffer";
			reg = <0 0xF1000000 0 (1080 * 2400 * 4)>;
			width = <1080>;
			height = <2400>;
			stride = <(720 * 4)>;
			format = "a8r8g8b8";
			status= "okay";
		};
    };

};